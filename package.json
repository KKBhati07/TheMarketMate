{
  "name": "marketmate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "serve:ssr:marketmate": "node dist/marketmate/server/server.mjs",

    "analyze:marketmate": "nx build marketmate --configuration production --source-map && source-map-explorer \"dist/marketmate/browser/*.js\"",
    "analyze:marketmate:dev": "nx build marketmate --configuration development --source-map && source-map-explorer \"dist/marketmate/browser/*.js\"",
    "analyze:admin": "nx build mm-admin-portal --configuration production --source-map && source-map-explorer \"dist/mm-admin-portal/browser/*.js\"",
    "analyze:all": "npm run analyze:marketmate && npm run analyze:admin"
  },
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.0",
    "express": "^4.18.2",
    "ngx-cookie-service": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular-devkit/core": "^18.2.0",
    "@angular-devkit/schematics": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@nx/angular": "22.2.3",
    "@nx/eslint": "22.2.3",
    "@nx/workspace": "22.2.3",
    "@schematics/angular": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.0",
    "nx": "22.2.3",
    "typescript": "~5.5.2",
    "source-map-explorer": "^2.5.3"
  }
}
