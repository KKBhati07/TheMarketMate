{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "marketmate",
  "projectType": "application",
  "tags": [
    "type:app"
  ],
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/marketmate/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/marketmate",
        "index": "apps/marketmate/src/index.html",
        "browser": "apps/marketmate/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/marketmate/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/marketmate/public"
          }
        ],
        "styles": [
          "apps/mm-shared/src/styles/styles.scss",
          "apps/marketmate/src/styles.scss"
        ],
        "scripts": [],
        "server": "apps/marketmate/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/marketmate/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kB",
              "maximumError": "1MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kB",
              "maximumError": "4kB"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production",
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "marketmate:build:production"
        },
        "development": {
          "buildTarget": "marketmate:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n"
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "apps/marketmate/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/marketmate/public"
          }
        ],
        "styles": [
          "apps/mm-shared/src/styles/styles.scss",
          "apps/marketmate/src/styles.scss"
        ],
        "scripts": []
      }
    }
  }
}